<%= render 'shared/title', title: 'Post List'%>
 <% @posts.each do |post| %>
<div>
    <div class="row">
    <div class="well col-md-6">
      <p>
        <h4><%= post.title %></h4>
      </p>
      <p>
        <%= post.content %>
      </p>
      <br>
    </div>
   </div>
     <div style="margin-bottom:30px;">
     <%= link_to "show this post", post_path(post)%>
      </div>
</div>
 <% end %>